
public class Graduate extends Student{
	private String lab;
	
	void setLab(String lab) { this.lab = lab; }
	String getLab() { return lab; }
	
	public Graduate() {
		super();
		setLab(" ");
	}
	
	public Graduate(int id, int tuition, double gpa, String lab) {
		setId(id);
		setTuition(tuition);
		setGpa(gpa);
		setLab(lab);
	}
}
