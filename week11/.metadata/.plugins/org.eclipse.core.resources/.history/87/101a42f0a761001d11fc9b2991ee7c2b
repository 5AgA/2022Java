
public class BoxTest {

	public static void main(String[] args) {
		Box b0 = new Box();
		Box b1 = new Box();
		Box b2 = new Box(100, 60, 20);
		
		b1.setX(30);
		b1.setY(45);
		b1.setZ(53);
		b1.fillBox();
		
		System.out.println("디폴트 상자의 정보입니다." + b0);
		System.out.println("디폴트 상자의 정보입니다." + b1);
		System.out.println("디폴트 상자의 정보입니다." + b2);
	}

}
