
public class Training2 {

	public static void main(String[] args) {
		int i;
		
		for (i = 0; i < 5; i++)
		{
			System.out.print((i * 3 + 1) + " ");
			if (i != 0 && i % 4 == 0)
				System.out.println(" ");
		}
		
		while (i < 34)
		{
			System.out.print((i * 3 + 1) + " ");
			if (i != 5 && i % 5 == 0)
				System.out.println(" ");
			i++;
		}

	}

}
